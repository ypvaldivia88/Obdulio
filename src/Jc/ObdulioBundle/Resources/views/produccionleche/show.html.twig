{% extends 'base.html.twig' %}

{% block body %}
    <h1>Produccionleche</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ produccionleche.id }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{% if produccionleche.fecha %}{{ produccionleche.fecha|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Valor</th>
                <td>{{ produccionleche.valor }}</td>
            </tr>
            <tr>
                <th>Factura</th>
                <td>{{ produccionleche.factura }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('produccionleche_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('produccionleche_edit', { 'id': produccionleche.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
