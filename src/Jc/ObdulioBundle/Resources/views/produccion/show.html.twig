{% extends 'base.html.twig' %}

{% block body %}
    <h1>Produccion</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ produccion.id }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{% if produccion.fecha %}{{ produccion.fecha|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Valor</th>
                <td>{{ produccion.valor }}</td>
            </tr>
            <tr>
                <th>Factura</th>
                <td>{{ produccion.factura }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('produccion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('produccion_edit', { 'id': produccion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
